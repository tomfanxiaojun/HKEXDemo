<!DOCTYPE html>
<html lang="en" class="market-hkex">
<head>
   {{>header title="ETP Education"}}
</head>
<body>
  {{> topbar}}
  <!-- end of topbar -->
  {{> navigation2}}
  <!-- end of nav -->
  <div id="skip-to-content"></div>
  <!-- end: anchor only for accessibility -->
  {{> etpOverViewBanner etpEducation}}
  <!-- end of banner section -->
  <div class="container content-container">
      <nav class="nav-side left-menu">
          <div class=" listing-committee-left-menu">
              {{> etpLeftMenu etpExchangeTraded='active' etpEducationMenu='active'}}
          </div>
          {{> monthlyBulletinBreadcrumb etpEducation}}
          <!-- breadcrumb -->
      </nav>
      <!-- end of nav side -->
      <main class="nav-content-wrap">
          <section class="etp-education-right">
              {{> educationRightPart}}
          </section>

          <section class="etp-education-loadMore">
            {{> loadMore}}
          </section>
      </main>
      <!-- end of main content -->
      <aside class="nav-side side-content">
          <div class="section__left_panel ">
              {{> etpOverViewSubscribe}}
          </div>
           <div class="section__left_panel ">
              {{> etpOverviewRelateNews}}
          </div>
           <div class="section__left_panel etp-overview">
              {{> etpOverviewReport}}
          </div>
      </aside>
    </div>

    <footer>
        <div class="container">
        {{> footer}}
        </div>
    </footer>

    {{> scripts}}

</body>
</html>
